   "source": [
    "def fourA(t, y, N):\n",
    "    S = 0\n",
    "    for k in range(N):\n",
    "        S += t[k]*y[k]\n",
    "    print(\"The sum is:\", S)\n",
    "    return S"
   ]
  },
